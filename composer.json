{
  "name": "marcosadantas/expressive-laravelmongodb",
  "description": "Provider laravel-mongodb lib to expressive service manager.",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Marcos Dantas",
      "email": "marcos.adantas@hotmail.com"
    }
  ],
  "require": {
    "illuminate/database": "5.5.2",
    "illuminate/pagination": "5.5.2",
    "jenssegers/mongodb": "3.3.1",
    "psr/container": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "ExpressiveLaravelMongodb\\": "src/"
    },
    "files": [
      "bootstrap.php"
    ]
  },
  "extra": {
    "zf": {
      "config-provider": "ExpressiveLaravelMongodb\\ConfigProvider"
    }
  }
}
